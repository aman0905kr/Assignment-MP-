   "source": [
    "import polars as pl\n",
    "\n",
    "# 1. Building LazyFrame over all CSVs and drop global duplicates\n",
    "lazy = (\n",
    "    pl.scan_csv(\"data/mock_csvs_final/*.csv\")\n",
    "      .unique(subset=[\"log_id\"])\n",
    ")\n",
    "\n",
    "# 2. writing it as a parquet file\n",
    "lazy.sink_parquet(\n",
    "    \"data/user_activity_combined.parquet\"\n",
    ")\n"
   ]
  }
